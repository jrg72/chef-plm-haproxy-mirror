{
  "name": "plm-website-dev",
  "default_attributes": {
    "plm-webapp": {
      "user": "www",
      "group": "www",
      "app_name": "plm",
      "background": true,
      "memcached": {
        "min_ratio": 0.1,
        "min_absolute_mb": 4096
      },
      "rails": {
        "env": "production"
      }
    },
    "plm-static": {
      "apache": {
        "listen": ["*:1234"]
      }
    },
    "plm-website-env": {
      "elastic_search": {
        "host": "127.0.0.1",
        "port": "9200",
        "namespace": "plm_production_alias"
      },
      "redis": {
        "host": "127.0.0.1",
        "port": "6379"
      },
      "redis-feeds": {
        "host": "127.0.0.1",
        "port": "6379"
      },
      "memcached_nodes": [
        "127.0.0.1"
      ],
      "resque": {
        "workers": [
          "\"\\\\\\\\*\""
        ]
      },
      "db": {
        "user": "patientowner",
        "host": "127.0.0.1",
        "database": "KIRJE"
      }
    },
    "plm-haproxy": {
      "user": "haproxy",
      "group": "haproxy",
      "ssl_dir": "/etc/pki/tls/private",
      "balance": "roundrobin",
      "proxies": [
        "HTTP",
        "app",
        "www-http",
        "www-https"
      ],
      "app": {
        "servers": [
          {
            "name": "app1",
            "address": "22.34.56.78"
          },
          {
            "name": "app2",
            "address": "22.34.56.79"
          }
        ],
        "port": "8080",
        "config": "maxconn 16 weight 50"
      }
    }
  }
}
